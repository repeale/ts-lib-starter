{
  "compilerOptions": {
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es6", "ESNext", "DOM"],
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "sourceMap": true,
    "strict": true,
    "target": "es5"
  },
  "include": ["./src/**/*"],
  "exclude": ["./src/**/*.test.ts"]
}
